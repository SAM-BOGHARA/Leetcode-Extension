{
    "manifest_version": 3,
    "name": "Leetcode Contest Language Identifier",
    "description": "This extension helps in identifying the language in which code is submitted on leetcode contests by participants",
    "version": "1.0",
    "icons": {
      "16": "images/LeetCode.png",
      "48": "images/LeetCode.png",
      "128": "images/LeetCode.png"
   },
    "action": {
        "default_popup": "popup/hello.html"
    },
    "permissions": [
        "activeTab",
        "storage" 
    ],
    "host_permissions": [
        "https://django-contest-api-yagffnekaq-uw.a.run.app/*"
    ],
    "background": {
        "service_worker": "background/background.js"
    },
    "content_scripts": [
        {
            "matches": [
                "https://leetcode.com/contest/weekly-contest-*/ranking/*",
                "https://leetcode.com/contest/weekly-contest-*/ranking/",
                "https://leetcode.com/contest/biweekly-contest-*/ranking/*",
                "https://leetcode.com/contest/biweekly-contest-*/ranking/"
            ],
            "js": ["scripts/content.js"],
            "run_at": "document_end"
        }
    ]
}
